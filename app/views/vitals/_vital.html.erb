<table class="table table-hover">
  <thead>
    <tr>
      <th>体温</th>
      <th>高血圧</th>
      <th>低血圧</th>
      <th>脈拍</th>
      <th>呼吸数</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <tr id="vital_<%= vital.id %>">
      <td>
        <%= vital.temperature %>
      </td>
      <td><%= vital.high_bp %></td>
      <td><%= vital.low_bp %></td>
      <td><%= vital.pulse %></td>
      <td><%= vital.breathing %></td>
    </tr>
  </tbody>
</table>